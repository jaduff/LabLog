@model ComputerModel
@{
    ViewData["Title"] = "Add Computer";
}


<h1>Add Room</h1>
<span class="alert">@ViewData["message"]</span>
<form asp-controller="Admin" asp-action="AddComputer" method="post">
    <div class="formgroup">
        <input asp-for="Name" class="form-control" placeholder="@(Model == null ? "Room Name" : @Model.Name)" />
        <button type="submit">Add Computer</button>
    </div>
</form>

@model List<RoomModel>
@{
    ViewData["Title"] = "Admin";
}

<h1>Admin</h1>
<h2>Rooms</h2>
<ul>
        @foreach (var room in Model)
        {
            <li>
                @room.Name
            </li>
        }

</ul>



<form action="./admin/AddRoom">
    <input type="submit" value="New Room" />
</form>